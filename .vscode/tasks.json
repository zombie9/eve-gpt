{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Spring Boot: Run Application",
			"type": "shell",
			"command": "mvn spring-boot:run",
			"isBackground": true,
			"problemMatcher": [
				"$msCompile"
			],
			"group": "build"
		},
		{
			"label": "Maven: Build Project",
			"type": "shell",
			"command": "mvn clean compile",
			"isBackground": false,
			"problemMatcher": [
				"$msCompile"
			],
			"group": "build"
		}
	]
}